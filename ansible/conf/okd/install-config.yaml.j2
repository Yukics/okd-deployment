apiVersion: v1
baseDomain: {{ searchdomain }}
compute: 
- hyperthreading: Enabled 
  name: worker
  replicas: 0 
controlPlane: 
  hyperthreading: Enabled 
  name: master
  replicas: 3 
metadata:
  name: {{ cluster_name }} 
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14 
    hostPrefix: 23 
  networkType: OVNKubernetes 
  serviceNetwork: 
  - 172.30.0.0/16
platform:
  none: {} 
pullSecret: '{{ redhat_pull_secret }}'
sshKey: '{{ ssh_public_key['content'] | b64decode | trim }}' 
capabilities:
  baselineCapabilitySet: vCurrent
  additionalEnabledCapabilities:
  - Build 
  - CSISnapshot
  - Console
  - ImageRegistry
  - Storage
  - marketplace
  - NodeTuning
  - OperatorLifecycleManager 